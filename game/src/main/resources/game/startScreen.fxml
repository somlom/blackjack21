<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.stage.Stage?>

<VBox fx:id="anchor" alignment="CENTER" spacing="20.0"
   xmlns="http://javafx.com/javafx/8.0.171"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.StartScreenController" styleClass="anchor" stylesheets="@global.css">
   <children>
      <Button fx:id="titel" onAction="#startGame" styleClass="titel" stylesheets="@global.css"/>
      <Button fx:id="startScreenButton" onAction="#startGame" styleClass="btn" stylesheets="@global.css"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
